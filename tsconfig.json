{
	"compilerOptions": {
		"baseUrl": ".",
		"jsx": "react",
		"module": "commonjs",
		"noImplicitAny": true,
		"outDir": "dist",
		"paths": {
			"*": ["node_modules/*"],
			"images/*": ["images/*"],
			"src/*": ["src/*"]
		},
		"sourceMap": true,
		"typeRoots": ["./node_modules/@types", "./types"]
	},
	"include": ["src/**/*"]
}
